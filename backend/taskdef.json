{
    "executionRoleArn": "REPLACE_ME_TASK_EXECUTION_ARN",
    "requiresCompatibilities": ["FARGATE"],
    "family": "mysfits-ecs-prod-backend",
    "memory": "512",
    "cpu": "256",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "backend",
        "image": "%IMAGE_URI%",
        "essential": true,
        "cpu": 0,
        "portMappings": [
          {
            "hostPort": 80,
            "protocol": "tcp",
            "containerPort": 80
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/mythicalmysfits/prod/backend",
            "awslogs-region": "%REGION%",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ]
  }